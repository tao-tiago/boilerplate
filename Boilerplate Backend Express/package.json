{
  "name": "services",
  "version": "0.0.1",
  "description": "The boilerplate microservice",
  "main": "src/server.ts",
  "engines": {
    "node": ">= 18.12.0",
    "npm": ">= 8.19.2"
  },
  "scripts": {
    "dev": "nodemon --ext '*' --exec 'node --inspect=0.0.0.0:9229 -r ts-node/register src/server.ts'",
    "build": "tsup src",
    "test": "jest --no-cache --passWithNoTests",
    "test:coverage": "jest --coverage --watchAll false --testResultsProcessor='jest-sonar-reporter'",
    "lint": "eslint . --ext .ts",
    "prepare": "if [ -f node_modules/.bin/husky ]; then husky install; fi"
  },
  "author": "Fiibo",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.16.1",
    "axios": "^1.6.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "tsyringe": "^4.7.0",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "eslint": "8.57.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.2.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "13.3.0",
    "nodemon": "3.1.4",
    "prisma": "5.16.1",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.1.0",
    "typescript": "5.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "jest --no-cache --passWithNoTests"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "reporter.xml",
    "indent": 4
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}